extends head

block content
    body
    
        nav
            ul
                a(href="/")
                    img(src="/images/nwa-fcc.svg", width="50px")
                li
                li Meet FCC-NWA
                li Vision
        
        div(id="output") 
            h1#eventTitle Next Event:
            p groupName: #{groupName}
            p name: #{name}
            //p description: #{description}
            //p eventURL: #{eventURL}
            p venueName: #{venueName}
            p venueAddress: #{venueAddress}
            p venueCity: #{venueCity}
            p venueState: #{venueState}
            p venueZip: #{venueZip}
            //p venueLatitude: #{venueLatitude}
            //p venueLongitude: #{venueLongitude}
            //p groupName: #{groupName}
            //p groupUrlName: #{groupUrlName}
            //p eventDuration: #{eventDuration} (this is in milliseconds -- it breaks down to 120 minutes)
            
            p eventWeekDay: #{eventWeekDay}
            p eventMonth: #{eventMonth}
            p eventDay: #{eventDay}
            p eventYear: #{eventYear}
            p eventStartHour: #{eventStartHour}
            p eventStartMin: #{eventStartMin}
            p eventStartPeriod: #{eventStartPeriod}
            
            //p venueLatitude: #{venueLatitude}
            //p venueLongitude: #{venueLongitude}
            //p groupUrlName: #{groupUrlName}
            p eventDuration: #{eventDuration} Hr